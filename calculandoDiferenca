programa {
	funcao inicio() {
	    inteiro numero1, numero2, numero3, numero4, diferenca
	    
		escreva("Digite numero 1: ")
		leia(numero1)
		
		escreva("Digite numero 2: ")
		leia(numero2)
		
		escreva("Digite numero 3: ")
		leia(numero3)
		
		escreva("Digite numero 4: ")
		leia(numero4)
		
		diferenca = (numero1 * numero2) - (numero3 * numero4)
		escreva(diferenca)
		
	}
}
